//Write a C program to demonstrate arithmetic operators (+, -, *, /, %).
#include<stdio.h>
int main() {
    int a, b;         //initializing numbers a and b
    printf("Enter the first number: ");
    scanf("%d", &a);    //storing value of a given by user
    printf("\n Enter the second number: ");
    scanf("%d", &b);    //storing value of b given by user
    //now printing their values by performing arithmetic operations
    printf("\n Addition: %d + %d = %d", a, b, a + b);
    printf("\n Subtraction: %d - %d = %d", a, b, a - b);
    printf("\n Multiplication: %d * %d = %d", a, b, a*b);
    printf("\n Division: %d / %d = %d", a, b, a/b);
    printf("\n Modulus: %d %% %d = %d", a, b, a %b);
    return 0;    //to show that program runs successfully
}
